# This tmux statusbar config was created by tmuxline.vim
# on Mon, 24 Feb 2014

set -g status-bg 'colour0'
set -g message-command-fg 'colour7'
set -g status-justify 'left'
set -g status-left-length '100'
set -g status 'on'
set -g pane-active-border-fg 'colour14'
set -g message-bg 'colour11'
set -g status-right-length '100'
set -g status-right-attr 'none'
set -g message-fg 'colour7'
set -g message-command-bg 'colour11'
set -g status-attr 'none'
#set -g status-utf8 'on'
set -g pane-border-fg 'colour11'
set -g status-left-attr 'none'
setw -g window-status-fg 'colour10'
setw -g window-status-attr 'none'
setw -g window-status-activity-bg 'colour0'
setw -g window-status-activity-attr 'none'
setw -g window-status-activity-fg 'colour14'
setw -g window-status-separator ''
setw -g window-status-bg 'colour0'
set -g status-left '#[fg=colour0,bg=colour11,bold] #S #[fg=colour11,bg=colour0,nobold,nounderscore,noitalics]'
set -g status-right '#[fg=colour237,bg=colour0,nobold,nounderscore,noitalics]#[fg=colour255,bg=colour237] %Y-%m-%d  %l:%M%p #[fg=colour11,bg=colour237,nobold,nounderscore,noitalics]#[fg=colour0,bg=colour11] #h '
setw -g window-status-format '#[fg=colour10,bg=colour0] #I #[fg=colour11,bg=colour0] #W '
setw -g window-status-current-format '#[fg=colour0,bg=colour237,nobold,nounderscore,noitalics]#[fg=colour255,bg=colour237] #I #[fg=colour255,bg=colour237] #W #[fg=colour237,bg=colour0,nobold,nounderscore,noitalics]'
